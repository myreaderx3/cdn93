                                                }
                                            });
                                        })
                                        .catch(err => console.error(err));
                                }

                                let paywallPicoObserver = new MutationObserver(mutationRecords => {
                                    mutationRecords.forEach((mutationRecord) => {
                                        setTimeout(() => {
                                            if (mutationRecord.target.dataset.picoStatus === 'paying') {
