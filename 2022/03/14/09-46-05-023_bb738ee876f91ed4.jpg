                                            }
                                        } else {
                                            if (av.length === (nov - 3)) {
                                                data = {
                                                    popupId: q('#modal-metered-paywall').dataset.popupFirstWarningId,
                                                    gtagEvent: 'popup_first_warning',
                                                    action: 'get_popup_content'
                                                };
                                                q('#modal-metered-paywall').classList.add('popup-first-warning');
                                            } else if (av.length === (nov - 1)) {
                                                data = {
                                                    popupId: q('#modal-metered-paywall').dataset.popupSecondWarningId,
                                                    gtagEvent: 'popup_second_warning',
                                                    action: 'get_popup_content'
                                                };
                                                q('#modal-metered-paywall').classList.add('popup-second-warning');
                                            }
                                            av.push(location.pathname);
                                            localStorage.setItem('av', JSON.stringify(av));
                                        }
                                    }
                                } else {
                                    av.push(location.pathname);
                                    localStorage.setItem('av', JSON.stringify(av));
                                    localStorage.setItem('cttl', (now.getTime() + ttl * 1000).toString());
                                }

                                if (data) {
                                    fetch(CTIPS.ajaxUrl, {
                                        method: 'POST',
                                        body: JSON.stringify(data)
                                    })
                                        .then(res => res.json())
                                        .then(res => {
