                                                    img.dataset.placeholderSrc = '';
                                                });
                                            }

                                            window.addEventListener('scroll', () => {
                                                if (q('#modal-metered-paywall') && q('#modal-metered-paywall').style.display === 'flex') {
                                                    window.scrollTo(0, pageYOffset);
                                                } else {
                                                    document.body.removeEventListener('touchmove', paywallTouchmoveListener);
                                                }
                                                if (isVisible(q('.article__content')) && !popup_paywall_show) {
                                                    pageYOffset = window.pageYOffset;
                                                    q('#modal-metered-paywall').style.display = 'flex';
                                                    setTimeout(() => {
                                                        q('#modal-metered-paywall .modal__content').style.transform = 'translateY(0)';
                                                        d.body.style.overflow = 'hidden';
                                                        // document.body.addEventListener('touchmove', paywallTouchmoveListener, {passive: false});
                                                        if (window.ga && !gtag_paywall_send) {
                                                            ga('send', 'event', 'Paywall', 'show_' + data.gtagEvent, 0, {'nonInteration': false});

                                                            if (qa('#modal-metered-paywall .PicoPopup_k8n44vr6').length) {
                                                                q('#modal-metered-paywall .PicoPopup_k8n44vr6').addEventListener('click', () => {
                                                                    ga('send', 'event', 'Paywall', 'signup_' + data.gtagEvent, 0, {'nonInteration': false});
                                                                });
                                                            }
                                                            if (qa('#modal-metered-paywall .PicoManageAccount').length) {
                                                                q('#modal-metered-paywall .PicoManageAccount').addEventListener('click', () => {
                                                                    ga('send', 'event', 'Paywall', 'login_' + data.gtagEvent, 0, {'nonInteration': false});
                                                                });
                                                            }
                                                            if (qa('#modal-metered-paywall .modal__close').length) {
                                                                q('#modal-metered-paywall .modal__close').addEventListener('click', () => {
