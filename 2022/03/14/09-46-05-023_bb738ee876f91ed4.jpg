                                                                (e) => {
                                                                    console.log('a returning user authenticated with event details: ', e.detail)
                                                                    console.log(e);
                                                                    ga('send', 'event', 'Paywall', 'user_authenticated_' + data.gtagEvent, 0, {'nonInteration': false});
                                                                });
                                                            document.addEventListener(
