                    function isGeoExclude() {

                        if ( /lighthouse|bot|googlebot|crawler|spider|robot|crawling/i.test(navigator.userAgent) ) {
                            return false;
                        }

                        try {

                            let exludeCountries = q('#modal-metered-paywall').dataset.exludeCountries ? JSON.parse(q('#modal-metered-paywall').dataset.exludeCountries) : '';

                            if (!exludeCountries) {
                                paywallInit();
                            } else {
                                showCountryCode()
                            }

                            async function showCountryCode() {
                                // let traceUrl = location.origin + "/cdn-cgi/trace";
                                let traceUrl = "https://cloudflare.com/cdn-cgi/trace";

                                let response = await fetch(traceUrl, {
                                    mode: "cors"
                                });

                                if (response.status === 200 && response.url === traceUrl) {
                                    let responseText = await response.text();
                                    if (responseText) {
                                        let data = responseText.replace(/[\r\n]+/g, '","').replace(/\=+/g, '":"');
                                        data = '{"' + data.slice(0, data.lastIndexOf('","')) + '"}';
                                        let countryCode = JSON.parse(data).loc;

                                        if (exludeCountries.indexOf(countryCode) === -1) {
                                            paywallInit();
                                        }
                                    }
                                } else {
                                    console.log('Not have a Country Code');
                                    paywallInit();
                                }
                            }
                        } catch (e) {
                            console.error(e);
                            paywallInit();
                        }
                    }
                </script>

