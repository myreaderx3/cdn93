                    function paywallInit() {
                        if (!isPicoLogin() || !isPicoPaying()) {
                            let now = new Date(),
                                nov = parseInt(4),
                                ttl = parseInt(2592000),
                                cttl = parseInt(localStorage.getItem('cttl') || 0),
                                av = JSON.parse(localStorage.getItem('av')) || [],
                                data,
                                pageYOffset,
                                popup_paywall_show = false,
                                gtag_paywall_send = false;

                            if (!av.includes(location.pathname)) {

                                if (av.length && cttl) {
                                    if (now.getTime() > cttl) {
                                        localStorage.removeItem('av');
                                        localStorage.removeItem('cttl');
                                    } else {
                                        if (av.length >= nov) {
                                            data = {
